{
  "name": "todolistreact",
  "version": "2.0.0",
  "homepage": ".",
  "proxy": "http://localhost:8000",
  "private": true,
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.3",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "vite": "^5.4.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "server": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "build": "react-scripts build && powershell -Command \"Copy-Item -Path './data/todos.php' -Destination './build/data/' -Force; Copy-Item -Path './data/settings.php' -Destination './build/data/' -Force; Copy-Item -Path './data/visitors.php' -Destination './build/data/' -Force; Copy-Item -Path './data/install.php' -Destination './build/' -Force; Copy-Item -Path './stats.php' -Destination './build/' -Force -ErrorAction SilentlyContinue\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15"
  }
}
